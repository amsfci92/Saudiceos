    <script src="~/Content/AdminResources/assets/global/plugins/morris/raphael-min.js" type="text/javascript"></script>
    <script src="~/Content/AdminResources/assets/global/plugins/counterup/jquery.waypoints.min.js" type="text/javascript"></script>
    <script src="~/Content/AdminResources/assets/global/plugins/counterup/jquery.counterup.min.js" type="text/javascript"></script>
    <script src="~/Content/AdminResources/assets/global/plugins/fullcalendar/fullcalendar.min.js" type="text/javascript"></script>
    <script src="~/Content/AdminResources/assets/global/plugins/flot/jquery.flot.min.js" type="text/javascript"></script>
    <script src="~/Content/AdminResources/assets/global/plugins/flot/jquery.flot.resize.min.js" type="text/javascript"></script>
