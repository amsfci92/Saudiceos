    <script src="~/Content/AdminResources/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="~/Content/AdminResources/assets/global/plugins/jquery.blockui.min.js" type="text/javascript"></script>
