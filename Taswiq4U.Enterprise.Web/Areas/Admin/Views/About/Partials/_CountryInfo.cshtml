@model Cigarette.Enterprise.ViewModels.ViewModels.About.AboutVM
@using Cigarette.Enterprise.ResourceManager.LanguagesResourceFiles

<div class="row">
    <div class="col-md-12">
        <table class="table table-striped table-bordered table-hover dt-responsive dataTable no-footer dtr-inline" width="100%" id="sample_4" cellspacing="0" role="grid" aria-describedby="sample_3_info" style="width: 100%;">

            <thead>
                <tr role="row">
                    <th class="all sorting_asc" tabindex="0" aria-controls="sample_3" rowspan="1" colspan="1" aria-sort="ascending" aria-label="First name: activate to sort column descending">@HomeResource.SmsCountryCode</th>
                    <th class="min-phone-l sorting" tabindex="0" aria-controls="sample_3" rowspan="1" colspan="1" aria-label="Last name: activate to sort column ascending">@HomeResource.AboutusFaceboo</th>
                    <th class="min-phone-l sorting" tabindex="0" aria-controls="sample_3" rowspan="1" colspan="1" aria-label="Last name: activate to sort column ascending">@HomeResource.AboutusPhone</th>
                    <th class="min-phone-l sorting" tabindex="0" aria-controls="sample_3" rowspan="1" colspan="1" aria-label="Last name: activate to sort column ascending">@HomeResource.AboutPhone2</th>
                    <th class=" sorting" tabindex="0" aria-controls="sample_3" rowspan="1" colspan="1" aria-label="Salary: activate to sort column ascending">*</th>
                </tr>
            </thead>
            <tbody>

                <tr role="row" class="odd">
                    <td class="sorting_1" tabindex="0">@Model.CountryId</td>
                    <td>@Model.FacebookPageLink</td>
                    <td>@Model.Phone</td>
                    <td>@Model.Phone2</td>
                    <td>
                        <a href="/Admin/About/Info/@Model.CountryId" class="btn btn-primary btn-sm">
                            <i class="fa fa-edit"></i>
                        </a>
                    </td>
                </tr>
            </tbody>

        </table>
    </div>

</div>

